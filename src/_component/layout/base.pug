doctype html
html(lang="en")
  head
    title= data.site.meta.title
    meta(
      content= data.site.meta.description 
      name="description"
    ) 

    meta(name="viewport", content="width=device-width, initial-scale=1.0")

    if config.entry.css.embed
      link(rel="stylesheet" href=`${embedPath}` embed)

  body      
    .content
      //- img(src="asset/image/wharehouse1.jpeg", alt="")
      block awesome-content

    if config.entry.css.external
      link(rel="stylesheet", href=`${baseUrl}style.css`)

script(src=`${baseUrl}asset/js/all.js`) 
